{
  "name": "confluence-wiki-mcp-server-extension",
  "displayName": "Confluence Wiki MCP Server",
  "version": "1.0.0",
  "publisher": "WindStyle",
  "description": "A VSCode/Cursor extension providing an MCP Server for Confluence Wiki integration",
  "author": {
    "name": "Windie Chai"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "keywords": [
    "confluence",
    "wiki",
    "mcp",
    "cursor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WindieChai/confluence-wiki-mcp-server-extension"
  },
  "icon": "resources/icon.png",
  "galleryBanner": {
    "color": "#001f3f",
    "theme": "dark"
  },
  "main": "./dist/extension/extension.js",
  "scripts": {
    "compile": "tsc -p ./ && copyfiles -f extension/config.enc dist/",
    "watch": "tsc -watch -p ./ && copyfiles -f extension/config.enc dist/",
    "package": "vsce package",
    "publish": "vsce publish",
    "test": "node ./dist/test/runTest.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.0",
    "confluence.js": "^1.7.4",
    "turndown": "^7.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/turndown": "^5.0.5",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^2.24.0",
    "copyfiles": "^2.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "confluence-wiki-mcp-server-extension",
          "title": "Confluence Wiki MCP Server",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "confluence-wiki-mcp-server-extension": [
        {
          "type": "webview",
          "id": "confluence-wiki-mcp-server-extension.configView",
          "name": "Configuration"
        }
      ]
    },
    "commands": [
      {
        "command": "confluence-wiki-mcp-server-extension.openConfig",
        "title": "Configuration",
        "category": "Confluence Wiki MCP Server"
      }
    ],
    "menus": {
      "extension/context": [
        {
          "command": "confluence-wiki-mcp-server-extension.openConfig",
          "group": "inline",
          "when": "extension == confluence-wiki-mcp-server-extension"
        }
      ]
    }
  }
}
